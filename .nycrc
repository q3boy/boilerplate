{
  "extension": [
    ".ts"
  ],
  "include": ["lib/**/*.ts"],
  "exclude": ["lib/*-skip-me.ts"],
  "reporter": [
    "text",
    "html"
  ],
  "cache": true,
  "sourceMap": true,
  "instrument": true,
  "compact": true,
  "check-coverage": true,
  "branches": 80,
  "lines": 90,
  "functions": 80,
  "statements": 90
}
